#:import random random


<ScreenRegister>:
    name:'ScreenRegister'
    Widget:
        canvas.before:
            Color:
                rgba: 0.2,0.3,0.4,1
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            pos: root.width * 0.1, root.height * 0.9
            size: root.width * 1, root.height * 0.05
            id: title
            text:"register tablet"
            font_size: 16
            color: 1,1,1,1

        Label:
            pos: root.width * 0.1, root.height * 0.8
            size: root.width * 0.2, root.height * 0.05
            id: tablet_id_label
            text:"tablet id"
            font_size: 16
            color: 1,1,1,1

        TextInput:
            id: tablet_id
            font_size: 16
            text:"0"
            pos: root.width * 0.3, root.height * 0.8
            size: root.width * 0.2, root.height * 0.05

        Label:
            pos: root.width * 0.1, root.height * 0.7
            size: root.width * 0.2, root.height * 0.05
            id: subject_id_label
            text:"subject id"
            font_size: 16
            color: 1,1,1,1

        TextInput:
            id: subject_id
            font_size: 16
            text:"0"
            pos: root.width * 0.3, root.height * 0.7
            size: root.width * 0.2, root.height * 0.05

        LoggedButton:
            id: tablet_id_btn
            name: 'register_tablet'
            background_color: 0.5,0.5,0.5,1
            background_normal: ''
            text: 'register tablet'
            font_size: 16
            size: root.width * 0.15, root.height * 0.05
            pos: root.width * 0.3, root.height * 0.6
            on_press: app.register_tablet()

        Label:
            pos: root.width * 0, root.height * 0.1
            size: root.width * 1, root.height * 0.05
            id: callback_label
            text:"callback"
            font_size: 16
            color: 1,1,1,1


