<ScreenSimulation>:
    name:'ScreenSimulation'
    Widget:
        canvas.before:
            Color:
                rgba: 0.2,0.3,0.4,1
            Rectangle:
                pos: self.pos
                size: self.size
        TimerLabel:
            pos: root.width * 0.01, root.height * 0.9
            size: root.width * 0.1, root.height * 0.05
            id: timer_time
            text:"000"
            font_name: "fonts/the_font.ttf"
            font_size: 48
            color: 1,1,1,1
        Label:
            pos: root.width * 0, root.height * 0.9
            size: root.width * 1, root.height * 0.05
            id: simulation_title
            text:"test"
            font_name: "fonts/the_font.ttf"
            font_size: 48
            color: 1,1,1,1

        GridLayout:
            cols:2
            spacing: '20dp'
            size: root.width * 0.5,root.height * 0.4
            pos: root.width * 0.25, root.height * 0.4


            LoggedSpinner:
                id: simulation_spinner_1
                name: "simulation_spinner_1"
                text: 'הייטה'
                font_size: 30
                font_name: "fonts/the_font.ttf"
                background_color: 0.2,0.2,0.2,1
                values: ('תומידקה תייטה','רושיאה תייטה','תרוקיב לבקל ישוקה לש הייטה')
                size: root.width * 0.15, root.height * 0.08
                pos: root.width * 0, root.height * 0.9

            Label:
                pos: root.width * 0, root.height * 0.9
                size: root.width * 1, root.height * 0.08
                id: label_spinner1
                text:"1 ןייסנ"
                font_name: "fonts/the_font.ttf"
                font_size: 30
                color: 1,1,1,1

            LoggedSpinner:
                id: simulation_spinner_2
                name: "simulation_spinner_2"
                text: 'הייטה'
                font_size: 30
                font_name: "fonts/the_font.ttf"
                background_color: 0.2,0.2,0.2,1
                values: ('תומידקה תייטה','רושיאה תייטה','תרוקיב לבקל ישוקה לש הייטה')
